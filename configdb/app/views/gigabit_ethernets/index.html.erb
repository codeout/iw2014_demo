<%- model_class = GigabitEthernet -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:device_id) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:description) %></th>
      <th><%= model_class.human_attribute_name(:speed) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @gigabit_ethernets.each do |gigabit_ethernet| %>
      <tr>
        <td><%= link_to gigabit_ethernet.id, gigabit_ethernet_path(gigabit_ethernet) %></td>
        <td><%= gigabit_ethernet.device_id %></td>
        <td><%= gigabit_ethernet.name %></td>
        <td><%= gigabit_ethernet.description %></td>
        <td><%= gigabit_ethernet.speed %></td>
        <td><%=l gigabit_ethernet.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_gigabit_ethernet_path(gigabit_ethernet), :class => 'btn btn-primary btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      gigabit_ethernet_path(gigabit_ethernet),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-danger btn-xs' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_gigabit_ethernet_path,
            :class => 'btn btn-primary' %>
