<h1>Listing ipv6s</h1>

<table>
  <thead>
    <tr>
      <th>Gigabit ethernet</th>
      <th>Address</th>
      <th>Primary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ipv6s.each do |ipv6| %>
      <tr>
        <td><%= ipv6.gigabit_ethernet %></td>
        <td><%= ipv6.address %></td>
        <td><%= ipv6.primary %></td>
        <td><%= link_to 'Show', ipv6 %></td>
        <td><%= link_to 'Edit', edit_ipv6_path(ipv6) %></td>
        <td><%= link_to 'Destroy', ipv6, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ipv6', new_ipv6_path %>
